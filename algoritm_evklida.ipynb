    "import math\n",
    "\n",
    "a = 8\n",
    "b = 16\n",
    "\n",
    "def gspd(a, b):\n",
    "    while a != 0 and b != 0:\n",
    "        if a >= b:\n",
    "            a %= b\n",
    "        else:\n",
    "            b %= a\n",
    "    return a or b\n",
    "\n",
    "print (gspd(a, b))"
